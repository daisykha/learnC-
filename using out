using System;

namespace UsingOut
{
  class Program
  {
    static void Main(string[] args)
    {
      string statement = "GARRRR";
      Console.WriteLine(Whisper(statement, out bool marker));
    }  

    static string Whisper(string parameter1, out bool parameter2) {
      parameter1 = parameter1.ToLower();
      parameter2 = true;
      return parameter1, parameter2;
    }  
	}
}
